import json

def break_down_task(task_json):
    # Extract necessary fields from JSON
    task_parameters = task_json["task_parameters"]
    possible_subtasks = task_json["possible_subtasks"]
    subtask_parameters = task_json["subtask_parameters"]
    
    # List of parts to be processed
    parts_list = task_parameters["info"].split("材料：")[1].split("做法：")[0].strip().split("、")
    
    # Assembly instructions
    assembly_instructions = task_parameters["info"].split("做法：")[1].strip()
    
    # Generate a list of checkpoints
    check_points = [s.split("确认")[1].strip() for s in assembly_instructions.split("）") if "确认" in s]
    
    # Create a sequence of subtasks with parameters
    subtask_sequence = []
    for subtask in possible_subtasks:
        task = {"action": subtask, "parameters": {}}
        if subtask == "LocateParts" or subtask == "PickParts":
            task["parameters"]["partsList"] = parts_list
        elif subtask == "PlanAssembly":
            task["parameters"]["assemblyInstructions"] = assembly_instructions
        elif subtask == "FinalCheck":
            task["parameters"]["checkPoints"] = check_points
        subtask_sequence.append(task)
    
    # Return the subtask sequence in JSON format
    return json.dumps({"subtask_sequence": subtask_sequence}, ensure_ascii=False, indent=4)

# Load task details from JSON
task_json = json.loads(input_json)

# Break down the task into a sequence of subtasks
output_json = break_down_task(task_json)

print(output_json)
